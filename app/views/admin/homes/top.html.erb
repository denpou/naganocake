<h4>管理者ログイン</h4>

<%= form_with model: @user, url: user_session_path, local: true do |f| %>
  <div class="field">
   メールアドレス <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>
  <br>
  <div class="field">
  パスワード  <%= f.password_field :password, autocomplete: "current-password" %>
  </div>
  <br>
  <div class="actions">
    <%= f.submit "ログイン" %>
  </div>
<% end %>